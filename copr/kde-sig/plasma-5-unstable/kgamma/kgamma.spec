Name:    kgamma
Summary: A monitor calibration tool 
Epoch:   1
Version: 5.5.95
Release: 1%{?dist}

License: GPLv2
URL:     https://projects.kde.org/projects/kde/workspace/kgamma5

%global revision %(echo %{version} | cut -d. -f3)
%if %{revision} >= 50
%global stable unstable
%else
%global stable stable
%endif
Source0: http://download.kde.org/%{stable}/plasma/%{version}/kgamma5-%{version}.tar.xz

BuildRequires: cmake
BuildRequires: desktop-file-utils
BuildRequires: gettext
BuildRequires: extra-cmake-modules
BuildRequires: kf5-rpm-macros
BuildRequires: kf5-kdelibs4support-devel
BuildRequires: kf5-kdoctools-devel
BuildRequires: kf5-ki18n-devel
BuildRequires: pkgconfig(xxf86vm) 
BuildRequires: pkgconfig(Qt5Gui) pkgconfig(Qt5Widgets)
BuildRequires: pkgconfig(Qt5X11Extras)

Provides: kgamma5 = %{version}-%{release}

# when split occurred
Conflicts: kdegraphics < 7:4.6.95-10


%description
%{summary}.


%prep
%setup -q -n kgamma5-%{version}


%build
mkdir %{_target_platform}
pushd %{_target_platform}
%{cmake_kf5} ..
popd

make %{?_smp_mflags} -C %{_target_platform}


%install
make install/fast DESTDIR=%{buildroot} -C %{_target_platform}

%find_lang kcmkgamma


%files -f kcmkgamma.lang
%doc AUTHORS ChangeLog
%license COPYING
%{_kf5_datadir}/kgamma/
%{_kf5_datadir}/kservices5/kgamma.desktop
%{_qt5_plugindir}/kcm_kgamma.so
%{_kf5_docdir}/HTML/en/kgamma5/


